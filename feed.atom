---
layout: nil
---

<?xml version="1.0" encoding="utf-8"?>
   <feed xmlns="http://www.w3.org/2005/Atom">
	 <title>blog.bbcoimbra.com</title>
	 <link href="http://blog.bbcoimbra.com/feed.atom" rel="self" />
	 <link href="http://blog.bbcoimbra.com/feed.atom" />
	 <updated>{{ site.time | date_to_xmlschema }}</updated>
	 <id>http://blog.bbcoimbra.com/</id>
	 <author>
		 <name>Bruno de Brito Coimbra</name>
		 <email>bbcoimbra@gmail.com</email>
	 </author>

	 {% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="http://blog.bbcoimbra.com{{post.url}}" />
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>http://blog.bbcoimbra.com{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	 {% endfor %}
	 </feed>